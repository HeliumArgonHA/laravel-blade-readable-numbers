{
    "name": "heliumargon/laravel-blade-readable-numbers",
    "description": "A blade directive for human readable numbers, k-style.",
    "keywords": [
        "heliumargon",
        "laravel-blade-readable-numbers",
        "laravel",
        "blade",
        "human-readable"
    ],
    "homepage": "https://github.com/HeliumArgonHA/laravel-blade-readable-numbers",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Tetzlaff",
            "email": "alex@heliumargon.net",
            "homepage": "https://heliumargon.net",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "^5|^6|^7",
        "php": "^7.1"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "SanderVanHooft\\ReadableNumbers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SanderVanHooft\\Tests\\ReadableNumbers\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SanderVanHooft\\ReadableNumbers\\ReadableNumbersServiceProvider"
            ]
        }
    }
}
